extends layout

block content
  h1= title
  p Welcome to #{title}
  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js', type='text/javascript')
  script(src='/javascripts/comm.js', type="text/javascript")
  div
   form(id='phonesensor')
   h2 Phone Sensor <br/>
    select(name='Graph',id='graph')
     option(value='avg', selected=true) Average
     option(value='wavg') Weight Avg
    select(name='Sensor',id='sensor')
     option(value='andro1', selected=true) Android 1
     option(value='andro1') Android 2
    select(name='SensorType',id='sensortype')
     option(value='ACL') Accelerometer
     option(value='VCCL') Audio Sensor
    input(type='text',name='starttime',id='starttime',placeholder='ddmmyyyyhhmmss',value='')
    input(type='text',name='endtime',id='endtime',placeholder='ddmmyyyyhhmmss',value='')
    input(type='submit',value='Submit',id='sendButton')
    <hr/>
    img(src='images/office_space.jpg',id='graphimage',alt='Visual Display')